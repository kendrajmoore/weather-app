<% include partials/header %>
<div class="container">
    <header class="jumbotron">
       <h1>Welcome to Yelp Campgrounds</h1>
    </header>
    <h1>This is the campgrounds page</h1>

    <a href="/campgrounds/new">Add a new Campground</a>

      <% campgrounds.forEach(function(campgrounds){ %>
          <div>

            <h4><%=campgrounds.name %></h4>
            <img src="<%= campgrounds.image %>">
         </div>
    <% }); %>
</div>

